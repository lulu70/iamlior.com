  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -H 0.0.0.0",
    "start": "yarn run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean && yarn start",
    "format": "prettier --write \"src/**/*.js\"",
    "type-check": "tsc --noEmit --watch",
    "ggl-types": "graphql-codegen",
    "ggl-types:watch": "graphql-codegen --config codegen.yml -w",
